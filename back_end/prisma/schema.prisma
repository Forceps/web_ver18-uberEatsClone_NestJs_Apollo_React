generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model restaurant {
  restaurant_id Int    @id @default(autoincrement())
  name          String
  isVegan       Int    @default(1)
  address       String
  ownerName     String
  categoryName  String
}
